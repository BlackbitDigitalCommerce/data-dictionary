{
    "name": "youwe/data-dictionary",
    "description": "A data dictionary for Pimcore",
    "type": "pimcore-bundle",
    "require": {
        "pimcore/pimcore": "^5.4",
        "graphp/graphviz": "^0.2.1",
        "clue/graph": "^0.9.0",
        "youwe/pimcore-object-bridge": "^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.4",
        "squizlabs/php_codesniffer": "^3.3"
    },
    "license": "gpl-3.0",
    "autoload": {
        "psr-4": {
            "Youwe\\DataDictionaryBundle\\": ".\/src"
        }
    },
    "extra": {
        "pimcore": {
            "bundles": [
                "Youwe\\DataDictionaryBundle"
            ]
        }
    },
    "authors": [
        {
            "name": "Paulo Vitor Bettini de Paiva Lima",
            "email": "p.bettini@youwe.nl"
        }
    ]
}
